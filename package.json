{
  "name": "bun-elysia-starter-kit",
  "version": "0.0.1",
  "scripts": {
    "test": "NODE_ENV=test bun test ./test/**.test.ts",
    "dev": "NODE_ENV=dev bun --env-file=.env run --watch app/index.ts",
    "cluster": "NODE_ENV=cluster bun --env-file=.env run app/cluster.ts",
    "cronjob": "NODE_ENV=cron bun --env-file=.env run app/crond.ts",
    "db": "database generate --schema database/mysql-client.prisma"
  },
  "dependencies": {
    "@eelkevdbos/elysia-basic-auth": "^1.4.1",
    "@elysiajs/bearer": "^1.1.1",
    "@elysiajs/cors": "^1.1.0",
    "@elysiajs/cron": "^1.1.0",
    "@elysiajs/jwt": "^1.1.0",
    "@elysiajs/static": "^1.1.0",
    "@elysiajs/swagger": "^1.1.0",
    "@faker-js/faker": "^8.4.1",
    "@prisma/client": "latest",
    "bullmq": "^5.12.0",
    "elysia": "^1.1.5",
    "elysia-i18next": "^3.0.1",
    "i18next": "^23.12.2"
  },
  "devDependencies": {
    "bun-types": "^1.1.21",
    "prisma": "latest"
  },
  "module": "app/index.js"
}
